{"remainingRequest":"/Users/gabriella/Documents/test4/data/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/gabriella/Documents/test4/data/src/App.vue?vue&type=style&index=0&lang=scss&","dependencies":[{"path":"/Users/gabriella/Documents/test4/data/src/App.vue","mtime":1608104333533},{"path":"/Users/gabriella/Documents/test4/data/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/gabriella/Documents/test4/data/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/gabriella/Documents/test4/data/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/gabriella/Documents/test4/data/node_modules/sass-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/gabriella/Documents/test4/data/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/gabriella/Documents/test4/data/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["App.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA","file":"App.vue","sourceRoot":"src","sourcesContent":["<template>\n    <div>\n        <notifications group=\"notifications\"\n                       @click=\"close\"\n                       duration=1000\n                       position=\"top left\">\n            <template slot=\"body\" slot-scope=\"props\">\n                <button class=\"close-btn\" @click=\"close(props.item.title)\"><img src=\"./assets/delete.svg\"/></button>\n                <div class=\"notification-container\">\n                    <p class=\"title\">{{props.item.title}}</p>\n                    <div v-html=\"props.item.text\"></div>\n                </div>\n            </template>\n        </notifications>\n        <Diagram :json=\"data\" :class=\"{landscape: landscape.length}\" @click-node=\"clickNode\"/>\n    </div>\n</template>\n\n<script>\n  import Diagram from \"@/components/Diagram\";\n  import axios from 'axios';\n\n  export default {\n    name: 'app',\n    components: {\n      Diagram\n    },\n    data() {\n      return {\n        flattenData: [],\n        landscape: [],\n        data: {}\n      };\n    },\n    methods: {\n      clickNode: function (node) {\n        this.close();\n        this.setSelected(node.name);\n        this.$notify({\n          group: 'notifications',\n          title: node.name,\n          text: node.description,\n        });\n      },\n      nest(items, name = '', link = 'parent') {\n        return items.filter(item => item[link] === name)\n            .map(item => ({\n              name: item.name,\n              description: item.description,\n              class: item.class || '',\n              children: this.nest(items, item.name),\n              duration: 100000,\n            }));\n      },\n      setSelected(name, selected = true) {\n        this.flattenData.forEach(item => {\n          if (item.name === name) {\n            item.class = selected ? 'selected' : '';\n          } else {\n            item.class = '';\n          }\n        });\n        const process = this.nest(this.flattenData);\n        this.data = {\n          name: process[0].name,\n          description: process[0].description,\n          class: [\"rootNode\", process[0].class],\n          children: process[0].children\n        };\n      },\n      close: function (name) {\n        this.$notify({group: 'notifications', clean: true});\n        if (name) {\n          this.setSelected(name, false);\n        }\n      }\n    },\n    mounted() {\n      axios.get('https://us-central1-service-e063e.cloudfunctions.net/app')\n          .then(response => {\n            this.flattenData = response.data;\n            const data = this.nest(response.data);\n            this.data = {\n              name: data[0].name,\n              description: data[0].description,\n              class: [\"rootNode\"],\n              children: data[0].children\n            };\n          })\n          .catch(error => {\n            console.log(error)\n            this.errored = true\n          })\n          .finally(() => this.loading = false)\n    }\n  }\n</script>\n\n<style lang=\"scss\">\n    body {\n        font-family: 'Avenir', Helvetica, Arial, sans-serif;\n        -webkit-font-smoothing: antialiased;\n        -moz-osx-font-smoothing: grayscale;\n        padding: 0;\n        padding-top: 80px;\n        height: 300px;\n        margin: 0;\n\n        h2 {\n            font-weight: 300;\n\n            a {\n                color: black;\n                font-size: 12px;\n            }\n        }\n\n        #app {\n            text-align: center;\n            color: #2c3e50;\n\n            .content {\n                margin: 0 auto;\n            }\n        }\n    }\n\n    .vue-notification-wrapper {\n        min-width: 400px;\n        height: 120px;\n        background-color: white;\n        box-shadow: 0 1px 2px 0 rgba(0, 0, 0, 0.1), 0 3px 10px 0 rgba(0, 0, 0, 0.15);\n        border: 1px solid #cccccca1;\n        color: #2e776d;\n    }\n\n    .notification-container {\n        padding: 10px;\n    }\n\n    .close-btn {\n        background-color: #2e776d;\n        position: absolute;\n        left: 400px;\n        border: none;\n        width: 24px;\n        height: 24px;\n    }\n\n    .title {\n        font-size: 20px;\n        font-weight: 500;\n    }\n\n</style>\n"]}]}