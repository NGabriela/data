WApBA,uBAoBA,IApBA,EAoBA;AAAA,UAAA,QAAA,uEAAA,IAAA;AACA,WAAA,WAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,CAAA,IAAA,KAAA,IAAA,EAAA;AACA,UAAA,IAAA,CAAA,KAAA,GAAA,QAAA,GAAA,UAAA,GAAA,EAAA;AACA,SAFA,MAEA;AACA,UAAA,IAAA,CAAA,KAAA,GAAA,EAAA;AACA;AACA,OANA;AAOA,UAAA,OAAA,GAAA,KAAA,IAAA,CAAA,KAAA,WAAA,CAAA;AACA,WAAA,IAAA,GAAA;AACA,QAAA,IAAA,EAAA,OAAA,CAAA,CAAA,CAAA,CAAA,IADA;AAEA,QAAA,WAAA,EAAA,OAAA,CAAA,CAAA,CAAA,CAAA,WAFA;AAGA,QAAA,KAAA,EAAA,CAAA,UAAA,EAAA,OAAA,CAAA,CAAA,CAAA,CAAA,KAAA,CAHA;AAIA,QAAA,QAAA,EAAA,OAAA,CAAA,CAAA,CAAA,CAAA;AAJA,OAAA;AAMA,KAnCA;AAoCA,IAAA,KAAA,EAAA,eAAA,IAAA,EAAA;AACA,WAAA,OAAA,CAAA;AAAA,QAAA,KAAA,EAAA,eAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA;;AACA,UAAA,IAAA,EAAA;AACA,aAAA,WAAA,CAAA,IAAA,EAAA,KAAA;AACA;AACA;AAzCA,GAdA;AAyDA,EAAA,OAzDA,qBAyDA;AAAA;;AACA,SAAA,OAAA,GAAA,IAAA;AACA,IAAA,KAAA,CAAA,GAAA,CAAA,0DAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,MAAA,MAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA;;AACA,UAAA,IAAA,GAAA,MAAA,CAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA;;AACA,MAAA,MAAA,CAAA,IAAA,GAAA;AACA,QAAA,IAAA,EAAA,IAAA,CAAA,CAAA,CAAA,CAAA,IADA;AAEA,QAAA,WAAA,EAAA,IAAA,CAAA,CAAA,CAAA,CAAA,WAFA;AAGA,QAAA,KAAA,EAAA,CAAA,UAAA,CAHA;AAIA,QAAA,QAAA,EAAA,IAAA,CAAA,CAAA,CAAA,CAAA;AAJA,OAAA;AAMA,KAVA,EAWA,KAXA,CAWA,YAAA;AACA,MAAA,MAAA,CAAA,OAAA,CAAA;AAAA,QAAA,KAAA,EAAA,QAAA;AAAA,QAAA,KAAA,EAAA,OAAA;AACA,QAAA,IAAA,EAAA,oDADA;AACA,QAAA,IAAA,EAAA;AADA,OAAA;AAEA,KAdA,EAeA,OAfA,CAeA;AAAA,aAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AAAA,KAfA;AAgBA;AA3EA,CAAA","sourcesContent":["<template>\n    <div class=\"container\">\n        <div v-if=\"loading\" class=\"loading\"/>\n        <notifications type=\"error\" group=\"errors\"/>\n        <notifications @click=\"close\"\n                       duration=1000\n                       group=\"notifications\"\n                       class=\"info-notification\"\n                       position=\"top left\">\n            <template slot=\"body\" slot-scope=\"props\">\n                <button class=\"close-btn\" @click=\"close(props.item.title)\"><img src=\"./assets/delete.svg\" alt=\"delete\"/></button>\n                <div class=\"notification-container\">\n                    <p class=\"title\">{{props.item.title}}</p>\n                    <div v-html=\"props.item.text\"></div>\n                </div>\n            </template>\n        </notifications>\n        <Diagram :json=\"data\" :class=\"{landscape: landscape.length}\" @click-node=\"clickNode\"/>\n    </div>\n</template>\n\n<script>\n  import Diagram from \"@/components/Diagram\";\n  import axios from 'axios';\n\n  export default {\n    name: 'app',\n    components: {\n      Diagram\n    },\n    data() {\n      return {\n        flattenData: [],\n        landscape: [],\n        data: {},\n        error: false,\n        loading: false,\n      };\n    },\n    methods: {\n      clickNode: function (node) {\n        this.close();\n        this.setSelected(node.name);\n        this.$notify({\n          group: 'notifications',\n          title: node.name,\n          text: node.description,\n        });\n      },\n      nest(items, name = '', link = 'parent') {\n        return items.filter(item => item[link] === name)\n            .map(item => ({\n              name: item.name,\n              description: item.description,\n              class: item.class || '',\n              children: this.nest(items, item.name),\n              duration: 100000,\n            }));\n      },\n      setSelected(name, selected = true) {\n        this.flattenData.forEach(item => {\n          if (item.name === name) {\n            item.class = selected ? 'selected' : '';\n          } else {\n            item.class = '';\n          }\n        });\n        const process = this.nest(this.flattenData);\n        this.data = {\n          name: process[0].name,\n          description: process[0].description,\n          class: [\"rootNode\", process[0].class],\n          children: process[0].children\n        };\n      },\n      close: function (name) {\n        this.$notify({group: 'notifications', clean: true});\n        if (name) {\n          this.setSelected(name, false);\n        }\n      }\n    },\n    mounted() {\n      this.loading = true;\n      axios.get('https://us-central1-service-e063e.cloudfunctions.net/app')\n          .then(response => {\n            this.flattenData = response.data;\n            const data = this.nest(response.data);\n            this.data = {\n              name: data[0].name,\n              description: data[0].description,\n              class: [\"rootNode\"],\n              children: data[0].children\n            };\n          })\n          .catch(() => {\n            this.$notify({group: 'errors', title: 'Error',\n              text: 'An error has occurred and data could not be loaded', type: 'error'});\n          })\n          .finally(() => this.loading = false)\n    }\n  }\n</script>\n\n<style lang=\"scss\">\n.container{\n    .info-notification{\n        body {\n            font-family: 'Avenir', Helvetica, Arial, sans-serif;\n            -webkit-font-smoothing: antialiased;\n            -moz-osx-font-smoothing: grayscale;\n            padding: 0;\n            padding-top: 80px;\n            height: 300px;\n            margin: 0;\n\n            h2 {\n                font-weight: 300;\n\n                a {\n                    color: black;\n                    font-size: 12px;\n                }\n            }\n\n            #app {\n                text-align: center;\n                color: #2c3e50;\n\n                .content {\n                    margin: 0 auto;\n                }\n            }\n        }\n        .vue-notification-wrapper {\n            min-width: 400px;\n            height: 120px;\n            background-color: white;\n            box-shadow: 0 1px 2px 0 rgba(0, 0, 0, 0.1), 0 3px 10px 0 rgba(0, 0, 0, 0.15);\n            border: 1px solid #cccccca1;\n            color: #2e776d;\n        }\n\n        .notification-container {\n            padding: 10px;\n        }\n\n        .close-btn {\n            background-color: #2e776d;\n            position: absolute;\n            left: 400px;\n            border: none;\n            width: 24px;\n            height: 24px;\n        }\n\n        .title {\n            font-size: 20px;\n            font-weight: 500;\n        }\n    }\n    position: relative;\n    .loading {\n        color: #2e776d;\n        position: fixed;\n        top:20px;\n        left: 50%;\n        border: 16px solid #f3f3f3;\n        border-top: 16px solid #2e776d;\n        border-radius: 50%;\n        width: 60px;\n        height: 60px;\n        animation: spin 2s linear infinite;\n    }\n    @keyframes spin {\n        0% { transform: rotate(0deg); }\n        100% { transform: rotate(360deg); }\n    }\n}\n\n</style>\n"],"sourceRoot":"src"}]}